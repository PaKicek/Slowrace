func int factorial (int n) { // up to 20!
    int result = 1;
    for (int i = 2; i <= n; i++) {
        result = result * i;
    }
    return result;
}

main (int argc, array string argv[]) {
    if (argc >= 1) {
        int n = to_int(argv[0]);
        int result = factorial(n);
        print("Factorial of the number ");
        print(n);
        print(" is ");
        println(result);
    } else {
        println("No number is passed");
    }
}